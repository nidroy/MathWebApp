@{
    ViewData["Title"] = "Read Counterparty Page";
}

@model counterparty

<h3 style="margin-bottom: 15px;">Справочник контрагента</h3>

<table border="1">
    <tr>
        <td>Имя</td>
        <td>@Model.first_name</td>
    </tr>
    <tr>
        <td>Фамилия</td>
        <td>@Model.last_name</td>
    </tr>
    <tr>
        <td>Телефон</td>
        <td>@Model.phone</td>
    </tr>
    <tr>
        <td>Электронная почта</td>
        <td>@Model.email</td>
    </tr>
</table>